{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "babel-plugin-styled-components",
            {
                "minify": false,
                "uglifyPure": false
            }
        ]
    ],
    "env": {
        "production": {
            "ignore": [
                "**/*.test.jsx",
                "**/*.spec.jsx",
                "**/*.snap"
            ]
        }
    }
}
