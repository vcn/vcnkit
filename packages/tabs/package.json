{
    "name": "@vcnkit/tabs",
    "version": "0.4.1",
    "description": "Material Tabs bar for VCNkit",
    "author": "VCN Verzekerings Combinatie Nederland B.V.",
    "maintainers": [
        "Wouter Flohr <wflohr@vcn.nl>"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/vcn/vcnkit"
    },
    "bugs": "https://github.com/vcn/vcnkit/issues",
    "engines": {
        "node": ">8.0.0"
    },
    "scripts": {
        "build": "rimraf dist && babel ./src --out-dir ./dist --ignore '*.test.js,*.spec.js'",
        "clean": "rimraf dist"
    },
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "files": [
        "dist"
    ],
    "dependencies": {
        "@vcnkit/essentials": "^0.2.1",
        "@vcnkit/theme": "^2.2.1",
        "hoist-non-react-statics": "^3.3.2",
        "prop-types": "^15.8.1"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "styled-components": "^5.3.6"
    },
    "publishConfig": {
        "access": "public"
    },
    "jest": {
        "testURL": "http://localhost/",
        "moduleNameMapper": {
            "^react$": "<rootDir>/../../node_modules/react",
            "^react-test-renderer$": "<rootDir>/../../node_modules/react-test-renderer",
            "^react-dom$": "<rootDir>/../../node_modules/react-dom",
            "^styled-components$": "<rootDir>/../../node_modules/styled-components"
        }
    }
}
